{
    "name": "boxspaced/entity-manager",
    "require": {
        "php": "^5.5 || ^7.0",
        "zendframework/zend-db": "^2.8",
        "pimple/pimple": "^3.0",
        "mongodb/mongodb": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.2"
    },
    "suggest": {
        "mongodb/mongodb": "Persist your entities in MongoDB"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Boxspaced\\EntityManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boxspaced\\EntityManager\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1200
    }
}
