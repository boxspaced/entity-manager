{
    "name": "boxspaced/entity-manager",
    "require": {
        "php": ">=5.4",
        "zendframework/zend-db": "~2.8",
        "zendframework/zend-filter": "~2.7",
        "zendframework/zend-config": "~2.6",
        "pimple/pimple": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Boxspaced\\EntityManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boxspaced\\EntityManager\\Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1200
    }
}
